clc;
clear all;

% Importa os dados
load("tripartite.txt");

% Define início e término de cada matriz
linha_inicial = 1;
linha_final = 8;

%
parametro = -0.001;
for n = 1:1001
    estados = tripartite(linha_inicial:linha_final,1:8);
    tripartite1 = reshape(estados, 8, 8);
    estados_tripartite{n,1} = tripartite1;
    linha_inicial = linha_final + 1;
    linha_final = linha_final + 8;
    a = parametro + 0.001;
    a_peso(n,1) = a;
    parametro = a;
end
